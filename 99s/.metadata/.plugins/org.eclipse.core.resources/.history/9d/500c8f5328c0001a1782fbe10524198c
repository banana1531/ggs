<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="preresult">
	
	<select id="mypredict" resultType="com.ggs.DTO.PreResultDTO">
		SELECT gdate, hteamname, hscore, ateamname, ascore, predict, id 
	from teamRecord, preresult 
	where teamRecord.gno=preresult.gno AND id='test'
	</select>
	
</mapper>

